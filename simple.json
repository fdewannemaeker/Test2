{
    "version":8,
  
  "glyphs":"https://cdn.protomaps.com/fonts/pbf/{fontstack}/{range}.pbf",  
  
    "sources": {
        "protomaps": {
            "type": "vector",
            "url":  "https://alltiles.pmtilez.net/belgium/{z}/{x}/{y}.mvt",
                       
        }
    },
    "layers": [
        {
            "id":"buildings",
            "source": "protomaps",
            "source-layer":"landuse",
            "type": "fill",
            "paint": {
                "fill-color": "steelblue"
            }
        },
        {
            "id":"roads",
            "source": "protomaps",
            "source-layer":"roads",
            "type": "line",
            "paint": {
                "line-color": "black"
            }
        },
        {
            "id":"mask",
            "source": "protomaps",
            "source-layer":"mask",
            "type": "fill",
            "paint": {
                "fill-color": "white"
            }
        }
    ]
}
